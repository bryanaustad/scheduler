package org.lds.view.model;

/**
 * A JSON compatible POJO useful for sending error messages back to a service client.
 */
public class FieldError {

	private String id;
	private Object value;
	private String message;

	public FieldError(String id, String message) {
		this.id = id;
		this.message = message;
	}

	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}

	public Object getValue() {
		return value;
	}
	public void setValue(Object value) {
		this.value = value;
	}

	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
}
