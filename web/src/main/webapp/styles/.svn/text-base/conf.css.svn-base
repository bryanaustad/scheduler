/* Global */
* {
	margin: 0;
	padding: 0;
	position: relative;
}
body {
	background-color: #e9e8e8;
	font-family: arial, "lucida console", sans-serif;
}
a {
	outline: none
}
.clearfix {
	clear:both;
}

/* --- HEADER STUFF --- */
#session-d {
	background-color: #000;
	color: #fff;
	font-size: .75em;
	height: 38px;
}
#user-d {
	float: right;
	top: 12px;
	width: 12em;
}
.menu-d {
	float: right;
	top: 12px;
	/*width: 12em;*/
	margin-right: 20px;
}
#header-d {
	background-image: url("../images/bkg-header.png");
	color: #ccc;
	height: 38px;
}
#header-d h1{
	font-size: 1.25em;
	font-weight: normal;
	left: 12px;
	top: 12px;
	width: 36em;
}

/* --- PAGE STUFF --- */

#content-d {
	padding: 12px 44px;
	/* width: 1111px; */
}
#action-d {
	color: #666;
	font: 11px/12px arial;
	height: 30px;
}
#action-d div {
	cursor: pointer;
	float: left;
	height: 20px;
	padding: 5px;
	width: 125px;
}
#action-d div:hover {
	color: #333;
	text-shadow: #999 1px 1px 1px;
}
#action-d div span {
	left: 5px;
	top: -5px;
}

/* --- SEARCH STUFF --- */
#search-d {
	float: right;
	margin: 0 28px 0 0;
	top: 8px;
}
#search-d img, #search-d div {
	float: left;
	height: 64px;
	padding: 2px 4px;
	text-align: right;
}
#search-d input {
	background-image: url("../images/bkg-search-dark.png");
	border: none;
	color: #ccc;
	float: left;
	height: 21px;
	outline: none;
	padding: 5px 5px 0;
	width: 12em;
}
#search-d a:link, #search-d a:visited {
	color: #ccc;
	text-decoration: none;
}
#search-d a:hover {
	color: #ddd;
	text-decoration: underline;
}

/* --- LANDING PAGE STUFF --- */
#tech-desc {
	float: left;
	padding: 10px;
	width: 440px;
}
#tech-button {
	background-image:url('../images/landingPage/button-bkgrnd.png');
	background-repeat:no-repeat;
	width : 438px;
	height: 117px;
	cursor: default;
}
#tech-button:hover {
	cursor:pointer;
	background-image:url('../images/landingPage/button-bkgrnd-hover.png');
}
#tech-icon1-div {
	padding: 10px 10px 5px 15px;
	float: left;
}
#tech-icon2-div {
	padding: 18px 10px 5px 15px;
	float: left;
}
#tech-text-div {
	width: 300px;
	padding: 10px;
	float: left;
}
#almost-ready-d {
	position: absolute;
	width: 215px;
	height: 120px;
	top:0px;
	left: 100px;
}

/* --- SCHEDULE STUFF --- */
#calendar-d {
	float: left;
}

/* --- RESULTS STUFF --- */
#results-d {
	float: left;
}

/* --- FILTER STUFF --- */
#filters-d {
	float: left;
}

/* --- MODAL STUFF ---*/
#modal {
	width: 100%; height: 100%;
}
#modal-hdr {
	background-image: url(../images/bkg-black-grad-38.png);
	color: #fff;
	font: bold 16px/16px arial;
	width: 100%; height: 38px;
}
#modal-hdr span {
	top: 12px;
	left: 15px;
}
#modal-body, #modal-body2 {
	background-color: #000;
	color: #ccc;
	font: 10px/12px arial;
	overflow-y: auto;
	padding: 18px;
	height: 300px;
}
#modal-tiny {
	background-color: #000;
	color: #ccc;
	font: bold 16px/16px arial;
	overflow-y: auto;
	padding: 18px;
	height: 120px;
	text-align: center;
}
#modal-tiny img {
	padding: 26px
}
#modal-body div {
	float: left;
	padding: 2px;
	width: 143px;
}
#modal-body div:hover {
	color: #fff;
	text-decoration: underline;
}
#modal-body div span {
	color: #999;
}
#modal-body2 div {
	padding: 4px;
}
#modal-body2 div.name, #modal-body2 div span {
	font-size: 11px;
	font-weight: bold;
}
#modal-body2 div.register {
	color: #cdf;
	cursor: pointer;
	font-size: 14px;
	font-weight: bold;
}
#modal-body2 div.register:hover {
	color: #fff;
	text-decoration: underline;
}
#modal-ftr {
	background-image: url(../images/bkg-black-grad-12.png);
	width: 100%; height: 12px;
}
#simplemodal-container a.modalCloseImg, #simplemodal-container2 a.modalCloseImg {
	background:url(../images/white-x.png) no-repeat;
	width:8px;
	height:8px;
	display:inline;
	z-index:3200;
	position:absolute;
	top:13px;
	right:13px;
	cursor:pointer;
}
#simplemodal-container2 a.modalCloseImg {
	background: none;
}

/* GWT Dialog Style */
.dialogBox {
	border: none;
	/*padding: 10px 15px 15px 15px;*/
	padding: 0px;
	background: #000;
	z-index:15;
}
.dialogBox table, .dialogBox tr, .dialogBox td, .dialogBox th {
	border: none; margin: 0px; width: auto; padding: 0px;
}
.dialogTop {
	background-image: url(../images/bkg-header.png);
}
.dialogBottom {
}
.dialogBox .modalLinkWrapper {
	background-image: url(../images/bkg-header.png);
	height: 32px;
}
.dialogBox .modalLinkWrapper div {
	float: right;
	margin-right: 16px;
	margin-top: 8px;
}
.dialogBox .modalLinkWrapper div a {
	font-size: 12px;
	color: #DDD;
}
.dialogBox .Caption {
	/*font-size: 23px;*/
	/*color: #807C68;*/
	/*font-family: Palatino, "Palatino Linotype", Times, "Times New Roman", serif;*/
	/*font-style: inherit;*/
	/*font-weight: inherit;*/
	/*text-transform: inherit;*/
	/*text-decoration: inherit;*/
	/*margin-bottom: 10px;*/
	font-family:arial;
	font-size:16px;
	font-weight:bold;
	padding: 8px;
	color: #DDD;
	width: 700px;
}
.diaglogBackgroundShow {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 1080px;
	background: #222;
	opacity:.35;
	z-index:14;
}
.diaglogBackgroundHide {
	display: none;
}
.dialogBox .dialogContents {
	width: 700px;
	height: 300px;
	overflow-y: auto;
	padding: 8px;
	font-size: 12px;
	color: #DDD;
}
.dialogBox .dialogContents .dialogSessionTime {
	margin-bottom:8px;
}
.dialogBox .dialogContents .tagsModalColumn {
	float: left;
	width: 215px;
	margin-right: 20px;
}
.dialogBox .dialogContents .tagsModalColumnRight {
	float: left;
	margin-right: 0px;
}
.dialogBox .dialogContents .tagsModalColumn div, .dialogBox .dialogContents .tagsModalColumnRight div {
	margin-bottom: 8px;
}
.dialogBox .dialogContents .tagsModalColumn a, .dialogBox .dialogContents .tagsModalColumnRight a {
	font-size: 12px;
	color: #DDD;
}

.loadingModal img {
	margin: 75px 100px;
}
.loadingModal .modalCloseLink a {
	display:none;
}
.loadingModal .Caption {
	width: auto;
}
.loadingModal .dialogContents {
	width: auto;
	height: 200px;
}

