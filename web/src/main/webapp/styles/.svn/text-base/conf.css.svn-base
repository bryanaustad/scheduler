}

.rotateimg {
	vertical-align:middle;
	background: url('../images/bkg-table_header.gif') no-repeat center center fixed transparent;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
}
.rotatetext {
	line-height:18px;
	vertical-align:middle;
	display:block;
	font-weight: bold;
	font-size: 13px;
	color: #666666;
	vertical-align:middle;
	white-space:nowrap;
	/* Safari */
	-webkit-transform: rotate(-90deg);
	/* Firefox */
	-moz-transform: rotate(-90deg); /* FF3.5+ */
	-moz-transform-origin: 20px 0px;
	/* Internet Explorer */ filter: progidXImageTransform.Microsoft.BasicImage(rotation=3);
	-o-transform: rotate(-90deg);  /* Opera 10.5 */
	-webkit-transform: rotate(-90deg);  /* Saf3.1+, Chrome */
	filter:  progid:DXImageTransform.Microsoft.Matrix(sizingMethod='auto expand',
				M11=6.123031769111886e-17, M12=1, M21=-1, M22=6.123031769111886e-17); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Matrix(SizingMethod='auto expand',
				M11=6.123031769111886e-17, M12=1, M21=-1, M22=6.123031769111886e-17)"; /* IE8 */
	zoom: 1;
}

.outlinedashboard {
	border-left: 0px ;
	border-top: 0px ;
}
